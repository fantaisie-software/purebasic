Structure NETENT
  *n_name
  *n_aliases
  n_addrtype.w
  PB_Align(2,2)
  n_net.l
EndStructure

Structure SERVENT
  *s_name
  *s_aliases
  CompilerIf #PB_Compiler_Processor = #PB_Processor_x64
    *s_proto
    s_port.w
    PB_Align(2, 6)
  CompilerElse
    s_port.w
    PB_Align(2, 6)
    *s_proto
  CompilerEndIf
EndStructure

