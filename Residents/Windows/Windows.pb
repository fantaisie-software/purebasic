  sa_data.b[14]
EndStructure

Structure WSADATA
  wVersion.w
  wHighVersion.w
  CompilerIf #PB_Compiler_Processor = #PB_Processor_x86
    szDescription.a[#WSADESCRIPTION_LEN+1]
    szSystemStatus.a[#WSASYS_STATUS_LEN+1]
    iMaxSockets.u
    iMaxUdpDg.u
    PB_Align(2, 0)
    *lpVendorInfo
  CompilerElse                      ; #PB_Processor_x64
    iMaxSockets.u
    iMaxUdpDg.u
    *lpVendorInfo
    szDescription.a[#WSADESCRIPTION_LEN+1]
    szSystemStatus.a[#WSASYS_STATUS_LEN+1]
    PB_Align(0, 6)
  CompilerEndIf
EndStructure

Structure SOCKPROTO
  sp_family.w
  sp_protocol.w
EndStructure

Structure LINGER
  l_onoff.w
  l_linger.w
EndStructure

Structure HOSTENT
  *h_name
  *h_aliases
