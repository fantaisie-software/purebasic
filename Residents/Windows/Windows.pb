;
; A few macros (don't put a lot here, it can slow down the whole compiler)
;
Macro CreateWindow_(Class, Name, Style, x, y, W, H, Parent, Menu, Inst, Param)
  CreateWindowEx_(0, Class, Name, Style, x, y, W, H, Parent, Menu, Inst, Param)
EndMacro

Macro ImageList_AddIcon_(wnd, icon)
  ImageList_ReplaceIcon_(wnd, -1, icon)
EndMacro

; on x64, Rtl functions are always available as it's at least XP, so use them
;
Macro ZeroMemory_(a, b)
  RtlZeroMemory_(a, b)
EndMacro

Macro CopyMemory_(a, b, c)
  RtlMoveMemory_(a, b, c)
EndMacro

Macro MoveMemory_(a, b, c)
  RtlMoveMemory_(a, b, c)
EndMacro

Macro FillMemory_(a, b, c)
  RtlFillMemory_(a, b, c)
EndMacro

; RtlCopyMemory is mapped to memcpy() in Windows headers so we map it to RtlMoveMemory()
;
Macro RtlCopyMemory_(a, b, c)
  RtlMoveMemory_(a, b, c)
EndMacro

CompilerIf #PB_Compiler_Processor = #PB_Processor_x86
  ;
  ; These are real functions in x64, but don't exist in the x86 user32.dll,
  ; so redefine them with a macro like the C headers do
  ;
  Macro SetWindowLongPtr_(a, b, c)
    SetWindowLong_(a, b, c)
  EndMacro
  
  Macro GetWindowLongPtr_(a, b)
    GetWindowLong_(a, b)
  EndMacro
  
  Macro SetClassLongPtr_(a, b, c)
    SetClassLong_(a, b, c)
  EndMacro
  
  Macro GetClassLongPtr_(a, b)
    GetClassLong_(a, b)
  EndMacro
  
  #DWLP_MSGRESULT = 0
  #DWLP_DLGPROC   = 8
  #DWLP_USER      = 16
    
CompilerEndIf

CompilerIf #PB_Compiler_Processor = #PB_Processor_x64

  #DWLP_MSGRESULT = #DWL_MSGRESULT ; 0
  #DWLP_DLGPROC   = #DWL_DLGPROC   ; 4
  #DWLP_USER      = #DWL_USER      ; 8
  
  ; To ease the transition on x64
  ;
  Macro SetWindowLong_(a, b, c)
    SetWindowLongPtr_(a, b, c)
  EndMacro

  Macro GetWindowLong_(a, b)
    GetWindowLongPtr_(a, b)
  EndMacro

  Macro SetClassLong_(a, b, c)
    SetClassLongPtr_(a, b, c)
  EndMacro

  Macro GetClassLong_(a, b)
    GetClassLongPtr_(a, b)
  EndMacro

CompilerEndIf

